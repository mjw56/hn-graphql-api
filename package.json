{
  "name": "learning-graphql",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "npm run clean && babel src --optional runtime --out-dir build",
    "lint": "eslint src",
    "start": "node ./build/server/index.js",
    "prestart": "npm run build-server",
    "debug": "node --debug ./build/server/index.js"
  },
  "keywords": [
    "graphql"
  ],
  "author": "Mike Wilcox <mwilcox56@gmail.com> (http://github.com/mjw56)",
  "license": "MIT",
  "dependencies": {
    "dataloader": "^1.0.0",
    "express": "^4.13.3",
    "express-graphql": "^0.4.0",
    "graphql": "^0.4.7",
    "graphql-relay": "^0.3.2",
    "isomorphic-fetch": "^2.1.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-runtime": "^5.8.25",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.5.1"
  },
  "babel": {
    "optional": [
      "runtime",
      "es7.asyncFunctions"
    ]
  }
}
